*#
#***********************************************************************
*
*    HTML <head> section
*
*    Contains:
*    - Meta tags (charset, viewport, keywords, author, dates)
*    - Title
*    - Document head content
*    - Canonical link
*    - AI indexing (Markdown conversion, llms.txt)
*    - CSS stylesheets
*    - Google Tag Manager
*    - Schema.org metadata (JSON-LD)
*
***********************************************************************
*#
<head>
	<meta charset="$outputEncoding">
	<meta name="viewport" content="width=device-width, initial-scale=1" />

	<title>$shortTitle &ndash; $site.name</title>

#if ($keywords)
	<meta name="keywords" content="$keywords" />
#end
	<meta name="generator" content="$esc.html($generator)" />
#foreach ($author in $authors)
	<meta name="author" content="$author" />
#end
	<meta property="article:published_time" content="$date.format('iso', $publishDate)" />
	<meta property="article:modified_time" content="$date.format('iso', $publishDate)" />
##
## Header from the document
$headElement.html()
##
## Canonical link
#if ($project.url)
	<link rel="canonical" href="$PathTool.calculateLink($currentFileName, $project.url)">
#end
##
## AI Indexing: generate Markdown files for AI platforms and inserts a <link rel="alternate" /> in the head (if enabled)
#if ($buildAiIndex)
	$aiIndexTool.convertToMarkdown(${project.reporting.outputDirectory}, $currentFileName, $headElement, $bodyElement, $publishDate, $project.url)#*
	*##set($menuSection = "#getMenuSection()")#*
	*##call($aiIndexTool.updateLlmsTxt("${project.reporting.outputDirectory}/llms.txt", $currentFileName, $shortTitle, $site.name, $project.description, $menuSection, $project.url))#*
*##end
	<!-- build:css css/main-combined.css -->
	<link rel="stylesheet" href="../../../node_modules/bootstrap/dist/css/bootstrap.css">
	<link rel="stylesheet" href="../../../node_modules/@fortawesome/fontawesome-free/css/all.css">
	<link rel="stylesheet" href="../../../node_modules/angular-bootstrap-toggle/dist/angular-bootstrap-toggle.min.css">
	<link rel="stylesheet" href="css/prism-theme.css">
	<link rel="stylesheet" href="css/sentry.css">
	<link rel="stylesheet" href="css/copy-to-clipboard.css">
	<link rel="stylesheet" href="css/print.css">
	<!-- endbuild -->
	<link rel="stylesheet" href="$relativePath/css/site.css">
##
## Google Tag Manager
## Support both old location ($site.googleAnalyticsAccountId for Site 3.x) and new location in <custom> for Site 4.x
#set($googleAnalyticsId = $site.googleAnalyticsAccountId)#*
*##if (!$googleAnalyticsId)#set($googleAnalyticsId = $site.getCustomValue("googleAnalyticsAccountId"))#end#*
*##if ($googleAnalyticsId)
<script>(function(w,d,s,l,i){w[l]=w[l]||[];w[l].push({'gtm.start':
new Date().getTime(),event:'gtm.js'});var f=d.getElementsByTagName(s)[0],
j=d.createElement(s),dl=l!='dataLayer'?'&l='+l:'';j.async=true;j.src=
'https://www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);
})(window,document,'script','dataLayer','$googleAnalyticsId');</script>
#end
##
## schema.org metadata
<script type="application/ld+json">
{
	"@context": "https://schema.org",
	"@type": "TechArticle",
	"headline": "$esc.java($shortTitle)",
	"datePublished": "$date.format('iso', $publishDate)",
	"dateModified": "$date.format('iso', $publishDate)",
#if ($project.url)
	"mainEntityOfPage": "$esc.java($PathTool.calculateLink($currentFileName, $project.url))",
#end
#if (!$authors.isEmpty())
	"author": [
#foreach ($author in $authors)
		{
			"@type": "Person",
			"name": "$esc.java($author)"
		}#if ($foreach.hasNext),#end

#end
	]
#end
}
</script>
</head>
