<project name="${project.name}">

	<skin>
		<groupId>org.sentrysoftware.maven</groupId>
		<artifactId>sentry-maven-skin</artifactId>
		<version>${project.version}</version>
	</skin>

	<bannerLeft>
		<src>images/sentry-logo-114x40-white.png</src>
